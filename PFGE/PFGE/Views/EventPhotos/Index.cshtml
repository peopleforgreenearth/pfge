@model IEnumerable<PFGE.Models.EventPhoto>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/_MainPage.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.EventId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PhotoName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PhotoDescription)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.EventId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PhotoName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PhotoDescription)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.EventPhotoId }) |
            @Html.ActionLink("Details", "Details", new { id=item.EventPhotoId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.EventPhotoId })
        </td>
    </tr>
}

</table>
